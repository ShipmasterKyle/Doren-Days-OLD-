			<<set $trackedArousal[$trackedArousal.length - 1] += $args[0] * $breastsensitivity>>
		<</if>>
	<<else>>
		<<if $arousalError is undefined>>
			<<set $arousalError to [$args[0]]>>
		<<elseif $arousalError.length gte 50>>
			<<set $arousalError to [$args[0]]>>
		<<else>>
			<<set $arousalError.push($args[0])>>
		<</if>>
	<</if>>
<</nobr>><</widget>>

